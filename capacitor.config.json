{
  "appId": "com.eshop.gx",
  "appName": "ESHOP",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com",
        "twitter.com",
        "facebook.com"
      ],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": [
          "profile",
          "https://www.googleapis.com/auth/userinfo.email"
        ]
      }
    }
  },
  "cordova": {},
  "server": {
    "allowNavigation": [
      "*.firebaseapp.*"
    ]
  }
}
